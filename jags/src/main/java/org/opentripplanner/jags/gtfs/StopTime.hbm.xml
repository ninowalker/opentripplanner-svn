<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.opentripplanner.jags.gtfs">

    <class name="StopTime" table="StopTime">
		<id name="id" type="integer">
			<generator class="native"></generator>
		</id>
        <natural-id mutable="false">
        	<property name="trip_id"/>
        	<property name="stop_sequence"/>
		</natural-id>
		
        <component name="arrival_time">
            <property name="time" column="arrival_time"/>
        </component>

        <component name="departure_time">
            <property name="time" column="departure_time"/>
        </component>

        <property name="stop_id"/>
        <property name="stop_headsign"/>
        <property name="pickup_type"/>
        <property name="drop_off_type"/>
        <property name="shape_dist_traveled"/>
    </class>

</hibernate-mapping>